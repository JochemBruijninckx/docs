{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Automatic Rostering",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "getting-started/authentication",
              "getting-started/quickstart"
            ]
          },
          {
            "group": "Concepts",
            "pages": [
              "concepts/employees",
              "concepts/demands",
              "concepts/shifts",
              {
                "group": "Constraints",
                "icon": "shield-check",
                "pages": [
                  "constraints/index",
                  "constraints/employee-availability",
                  "constraints/employee-utilization",
                  "constraints/cooldown",
                  "constraints/consecutive",
                  "constraints/pattern",
                  "constraints/periodic-rest",
                  "constraints/period-distribution",
                  "constraints/prioritization",
                  "constraints/continuity",
                  "constraints/layout",
                  "constraints/rotation",
                  "constraints/fairness"
                ]
              }
            ]
          },
          {
            "group": "Other",
            "pages": ["misc/changelog", "misc/migrate"]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "api-reference/openapi.yaml"
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "http://localhost:3000/",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Playground",
          "href": "https://ar-playground-one.vercel.app/",
          "icon": "code"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/light.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact",
        "href": "mailto:stian.vale@visma.com"
      }
    ]
  },
  "contextual": {
    "options": ["copy", "chatgpt"]
  },
  "footer": {}
}
